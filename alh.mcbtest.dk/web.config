<?xml version="1.0" encoding="UTF-8"?>
<configuration>
   <system.webServer>
       <httpErrors>
           <remove statusCode="500" subStatusCode="-1" />
           <remove statusCode="500" subStatusCode="100" />
           <remove statusCode="404" subStatusCode="-1" />
           <error statusCode="404" prefixLanguageFilePath="" path="/404.asp" responseMode="ExecuteURL" />
           <error statusCode="500" subStatusCode="100" prefixLanguageFilePath="" path="/500.asp" responseMode="ExecuteURL" />
           <error statusCode="500" prefixLanguageFilePath="" path="/500.asp" responseMode="ExecuteURL" />
       </httpErrors>
       <staticContent>
           <mimeMap fileExtension=".id" mimeType="text/plain"/>
       </staticContent>
   </system.webServer>
</configuration>
